{
  "name": "solana-memecoin-tracker",
  "version": "1.0.0",
  "description": "Claude Code agent for tracking and analyzing Solana memecoins from LetsBonk.fun using DexScreener data",
  "main": "main.js",
  "scripts": {
    "start": "node main.js",
    "dev": "node --watch main.js",
    "daily-report": "node src/reporters/daily-report.js",
    "weekly-report": "node src/reporters/weekly-report.js",
    "status": "node cli-commands.js status",
    "top": "node cli-commands.js top",
    "search": "node cli-commands.js search",
    "watchlist": "node cli-commands.js watchlist",
    "cleanup": "node cli-commands.js cleanup",
    "help": "node cli-commands.js help",
    "test": "node --test"
